{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "turbo build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "api/[...path].ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
